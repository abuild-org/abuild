declare BOOST_LIB_SUFFIX string = @BOOST_LIB_SUFFIX@
declare BOOST_SP_USE_PTHREADS local boolean = @BOOST_SP_USE_PTHREADS@
XLINKFLAGS = @PTHREAD_LIBS@
XCFLAGS = @PTHREAD_CFLAGS@
INCLUDES = @BOOST_INCLUDE@
LIBDIRS = @BOOST_LIBDIR@
if ($(BOOST_SP_USE_PTHREADS))
  XCPPFLAGS = -DBOOST_SP_USE_PTHREADS
endif
