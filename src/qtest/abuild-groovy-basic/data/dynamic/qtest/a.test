#!/usr/bin/env perl
require 5.008;
BEGIN { $^W = 1; }
use strict;

require TestDriver;

my $td = new TestDriver('demo');

$td->runtest("environment",
	     {$td->STRING => $ENV{'VAR1'} . " " . $ENV{'VAR2'} . "\n"},
	     {$td->STRING => "first value second value\n"},
	     $td->NORMALIZE_NEWLINES);

$td->report(1);
