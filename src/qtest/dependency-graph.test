#!/usr/bin/env perl
require 5.008;
BEGIN { $^W = 1; }
use strict;

chdir("dependency-graph") or die "chdir testdir failed: $!\n";

require TestDriver;

my $td = new TestDriver('DependencyGraph');

$td->runtest("full test",
	     {$td->COMMAND => "test_dependency_graph"},
	     {$td->FILE => "dependency-graph.out",
	      $td->EXIT_STATUS => 0},
	     $td->NORMALIZE_NEWLINES);

$td->report(1);
