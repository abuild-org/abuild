#!/usr/bin/env perl
require 5.008;
BEGIN { $^W = 1; }
use strict;

chdir("logger") or die "chdir testdir failed: $!\n";

require TestDriver;

my $td = new TestDriver('Logger');

$td->runtest("full test",
	     {$td->COMMAND => "test_logger"},
	     {$td->FILE => "logger.out",
	      $td->EXIT_STATUS => 0},
	     $td->NORMALIZE_NEWLINES);

$td->report(1);
